<%@user.created_events.each do |event|%>
<div class="event-container">
    <h4><%= event.id %></h4>
    <h5> <%= event.event_date %></h5>
</div>
<% end %>

<ul class="attended-events">
    <ul class="previous-events">
    <h3>"Previous events"</h3>
    <%@user.attended_events.where("event_date <?", Time.now).each do |event|%>
        <li class="past-event"><%=event.title%></li>
    <% end %>
    </ul>
    <ul class="upcoming-events">
    <h3>"Upcoming events"</h3>
    <%@user.attended_events.where("event_date > ?", Time.now).each do |event|%>
        <li class="upcoming-event"><%=event.title%></li>
    <% end %>
    </ul>
</ul>